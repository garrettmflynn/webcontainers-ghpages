  },
  "dependencies": {
    "@webcontainer/api": "^1.0.2",
    "express": "latest",
    "nodemon": "latest",
    "cors": "latest",
    "xterm": "^5.1.0",
    "xterm-addon-fit": "^0.7.0"
  }
}
